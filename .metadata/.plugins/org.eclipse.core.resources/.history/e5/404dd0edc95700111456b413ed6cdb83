package com.applicake.beanstalkclient.activities;

import com.applicake.beanstalkclient.Constants;
import com.applicake.beanstalkclient.Repository;

import android.content.SharedPreferences;
import android.os.Bundle;
import android.preference.PreferenceManager;
import android.support.v4.app.Fragment;

public class DeploymentsFragment extends Fragment {
  
  protected Repository repository;
  protected SharedPreferences prefs;
  
  @Override
  public void onCreate(Bundle savedInstanceState) {
    super.onCreate(savedInstanceState);
    this.repository = getActivity().getIntent().getParcelableExtra(Constants.REPOSITORY);
    this.prefs = PreferenceManager.getDefaultSharedPreferences(getActivity().getApplicationContext());

  }
}
